    <VisualElement class="adapter-row-data">
        <TextElement class="adapter-center-data ad-mob-id subtext" text=" e.g. ca-app-pub-xxxxxxxxxxxxxxxx~yyyyyyyyyy"/>
    </VisualElement>
</UXML>
